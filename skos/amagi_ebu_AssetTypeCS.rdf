    <skos:prefLabel xml:lang="en">Advertisement</skos:prefLabel>
    <skos:altLabel xml:lang="en">Commercial</skos:altLabel>
    <skos:altLabel xml:lang="en">Ad</skos:altLabel>
    <skos:definition>A commercial video is a short-form type of advertisement ideally 30 seconds
                long placed on TV, streaming services, social media platforms, and other digital channels. </skos:definition>
    <skos:inScheme rdf:resource="https://metadata.amagi.tv/skos/amagi_ebu_AssetTypeCS"/>
    <skos:broader rdf:resource="https://metadata.amagi.tv/skos/amagi_ebu_AssetTypeCS#_1.1"/>
    <skos:changeNote></skos:changeNote>
    <skos:historyNote>2023-08-27</skos:historyNote>
    <skos:note>Valid</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://metadata.amagi.tv/skos/amagi_ebu_AssetTypeCS#_1.1.3">
