<https://metadata.amagi.tv/skos/amagi_ebu_IdentifierTypeCS>
    a skos:ConceptScheme ;
    rdfs:isDefinedBy <https://metadata.amagi.tv/skos/amagi_ebu_IdentifierTypeCS> ;
    rdfs:label "EBU SKOS Classification Scheme for media IdentifierType"@en .


<https://metadata.amagi.tv/skos/amagi_ebu_IdentifierTypeCS#_tmsid>
    a ebucore:IdentifierType ;
    skos:changeNote """First version""" ;
    skos:definition """Gracenote TMSID. Check https://en.wikipedia.org/wiki/TMS_(entertainment_data) and https://www.gracenote.com/gracenote-id/"""@en ;
    skos:example "MV003331980000" ;
    skos:historyNote "2022-11-29" ;
    skos:inScheme <https://metadata.amagi.tv/skos/amagi_ebu_IdentifierTypeCS> ;
    skos:note "Valid" ;
    skos:prefLabel "TMSID"@en .


<https://metadata.amagi.tv/skos/amagi_ebu_IdentifierTypeCS#_eidr>
    a ebucore:IdentifierType ;
    skos:changeNote """First version""" ;
    skos:definition """The universal media identifier. Check https://www.eidr.org/"""@en ;
    skos:example "" ;
    skos:historyNote "2022-11-29" ;
    skos:inScheme <https://metadata.amagi.tv/skos/amagi_ebu_IdentifierTypeCS> ;
    skos:note "Valid" ;
    skos:prefLabel "EIDR"@en .

<https://metadata.amagi.tv/skos/amagi_ebu_IdentifierTypeCS#_IMDb>
