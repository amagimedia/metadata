      <rdf:type rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Role"/>
      <skos:prefLabel xml:lang="en">Assistant Unit Manager / Assistant Second Unit Manager / Unit
         Manager / Unit Production Manager</skos:prefLabel>
      <skos:definition xml:lang="en">Assist with the day-to-day financial operation of the shooting
         unit.</skos:definition>
