   <rdf:Description rdf:about="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20.5">
      <rdf:type rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Role"/>
      <skos:prefLabel xml:lang="en">Line Producer</skos:prefLabel>
      <skos:definition xml:lang="en">The line producer works with the production manager on
         operating and more particulaly financial aspects.Line Producer: He approves expenses,
         including locations, actors, and crew. </skos:definition>
      <skos:broader rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20"/>
