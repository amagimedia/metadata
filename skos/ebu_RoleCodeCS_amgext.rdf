   <rdf:Description rdf:about="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20.9">
      <rdf:type rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Role"/>
      <skos:prefLabel xml:lang="en">Assistant Producer</skos:prefLabel>
      <skos:definition xml:lang="en">Assistant Producers assist Producers in the production of
         programmes or series. They are responsible for defining the best format corresponding to
         the programme / film intention and targeted audience. Assistant Producers set up filming,
         find interviewees, archive material and locations, and on some productions they suggest and
         book studio guests. They supervise the post-production process, being involved in editing,
         dubbing and compilation. Note: not to be confused with Production Assistant. </skos:definition>
      <skos:broader rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20"/>
      <skos:inScheme rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS"/>
      <skos:changeNote xml:lang=""/>
      <skos:historyNote>2007-04-12</skos:historyNote>
