      <rdf:type rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Role"/>
      <skos:prefLabel xml:lang="en">Associate Producer</skos:prefLabel>
      <skos:definition xml:lang="en">The top assistant to the producer, usually the intermediary
         between the Producer and the shooting crew.</skos:definition>
      <skos:broader rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20"/>
