      <rdf:type rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Role"/>
      <skos:prefLabel xml:lang="en">Second Assistant Camera / Clap Boy / Clapper
         Loader</skos:prefLabel>
      <skos:definition xml:lang="en">Operates the clapperboard at the beginning of each take and
         loads the raw film stock into the camera magazines between takes. Also in charge of
         delivering film material to development labs, he oversees organization of camera equipment
         and transport of the equipment from one shooting location to another.</skos:definition>
      <skos:broader rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_6.2"/>
      <skos:inScheme rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS"/>
      <skos:changeNote xml:lang=""/>
