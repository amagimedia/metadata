   <rdf:Description rdf:about="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20.1">
      <rdf:type rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Role"/>
      <skos:prefLabel xml:lang="en">Producer</skos:prefLabel>
      <skos:definition xml:lang="en">A person who is responsible for the making of an audio-visual
         work, including business aspects, management of the productions, and the commercial success
         of the work within scheduled deadlines.</skos:definition>
      <skos:broader rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20"/>
      <skos:inScheme rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS"/>
      <skos:changeNote xml:lang=""/>
      <skos:historyNote>2007-04-12</skos:historyNote>
      <skos:example/>
      <skos:note>Valid</skos:note>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20.2">
      <rdf:type rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Role"/>
      <skos:prefLabel xml:lang="en">Executive Producer</skos:prefLabel>
      <skos:definition xml:lang="en">The person primarily in charge of the financial aspects of a
         overall production and who may also make major creative decisions. He is not involved in
         any technical aspects of the filmmaking process. Typically an executive producer handles
         business and legal issues. He may supervise one or more producers or
         co-producers.</skos:definition>
      <skos:broader rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20"/>
      <skos:inScheme rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS"/>
      <skos:changeNote xml:lang=""/>
      <skos:historyNote>2007-04-12</skos:historyNote>
      <skos:example/>
      <skos:note>Valid</skos:note>
   </rdf:Description>
   <rdf:Description rdf:about="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20.3">
      <rdf:type rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Role"/>
      <skos:prefLabel xml:lang="en">Supervising Producer</skos:prefLabel>
      <skos:definition xml:lang="en">The supervising producer coordinates the activities of more
         than one producer under the authority of the executive producer.</skos:definition>
      <skos:broader rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20"/>
      <skos:inScheme rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS"/>
