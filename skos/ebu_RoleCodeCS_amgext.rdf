   <rdf:Description rdf:about="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20.22.1">
      <rdf:type rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Role"/>
      <skos:prefLabel xml:lang="en">Vision Mixer / Vision Switcher</skos:prefLabel>
      <skos:definition xml:lang="en">The person responsible for mixing video feeds in a live
