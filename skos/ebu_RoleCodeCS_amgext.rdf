   <rdf:Description rdf:about="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20.39">
      <rdf:type rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Role"/>
      <skos:prefLabel xml:lang="en">Runner</skos:prefLabel>
      <skos:definition xml:lang="en">Runners are the most junior members of a television or film
         crew. The job can include holding back onlookers, getting coffee, answering phones in the
         production office, escorting actors to locations, acting as a stand-in while a short is
         worked out, or performing any other tasks helping the production in any
         way.</skos:definition>
      <skos:broader rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20"/>
      <skos:inScheme rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS"/>
