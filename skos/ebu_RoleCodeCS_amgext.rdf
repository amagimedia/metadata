   <rdf:Description rdf:about="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20.8">
      <rdf:type rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Role"/>
      <skos:prefLabel xml:lang="en">Segment Producer</skos:prefLabel>
      <skos:definition xml:lang="en">The segment producer is in charge of supervising the production
         of one or more segments of a programme or film</skos:definition>
      <skos:broader rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS#_20"/>
      <skos:inScheme rdf:resource="http://www.ebu.ch/metadata/ontologies/skos/ebu_RoleCodeCS"/>
