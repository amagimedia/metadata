@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ebucore: <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#> .
@prefix ebucoreext: <https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension> rdf:type owl:Ontology ;
                owl:imports <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore> .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#scopeNote>
    a rdf:Property, owl:AnnotationProperty ;
    rdfs:isDefinedBy <https://metadata.amagi.tv/ebu/amagi_ebucoreExtension> ;
    rdfs:label "scopeNote"@en ;
    skos:definition "A note to denote the scope of a grounded entity. Specifically useful to denote the manual-edits, active-meta"@en ;
    skos:scopeNote "This property may be used directly to annotate any owl:thing within ebucore scope"@en .

<http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#width>
        rdfs:range              <http://www.w3.org/2001/XMLSchema#integer> ;
        rdfs:domain             <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Image> .

<http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#height>
        rdfs:range              <http://www.w3.org/2001/XMLSchema#integer> ;
        rdfs:domain             <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Image> .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#isAttributedTo>
    a rdf:Property ;
    rdfs:range    <http://www.w3.org/2001/XMLSchema#string> ;
    rdfs:range    <http://www.w3.org/2001/XMLSchema#anyURI> ;
    rdfs:domain             ebucore:Asset ;
    rdfs:isDefinedBy <https://metadata.amagi.tv/ebu/amagi_ebucoreExtension> ;
    rdfs:label "SourceAttribution"@en ;
    skos:definition "This property allows to denote the attribution of an asset's metadata to its source url. The source urls could be media rss feed url or a source csv location."@en .

<http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#annotationId> 
    rdfs:range     <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Identifier> .      

# An asset either episode or feature will be associated with exactly one resource of class type AIAugmentedMeta
# The above constraint means that the urriId for AIAugmentedMeta instance will be equal to fn(asset_uri_id, constant) where
# asset_uri_id is the uriId of the asset with which the AIAugmentedMeta instance is associated. 
# In the below example
#   A744595 = fn(A1234, "AIMLAugmenedMeta"), fn could be md5sum hash
#
# Example of AIAugmentedMeta is as follows
# <https://compass.amagi.tv/gms/entity/A1234> a ebucore:Episode ;
#     ebucoreexr:hasAIAugmentedMeta <https://compass.amagi.tv/gms/entity/A1234#AIAugmentedMeta> .
#
# <https://compass.amagi.tv/gms/entity/A1234#AIAugmentedMeta> a ebucoreext:AIAugmentedMeta ;
#         ebucore:hasRelatedAsset <https://compass.amagi.tv/gms/entity/A1234>;
#         ebucoreext:hasAnnotation [ a ebucore:Annotation ;
#                                ebucore:annotationType ebucoreext:Entity ;
#                                ebucore:annotationName "Russia"@en ;
#                                ebucore:annotaionConfidence "3.4" ;
#                                ebucore:annotationId [ 
#                                       a ebucore:Identifier ;
#                                       ebucore:identifierValue "Q34266" ;
#                                       ebucore:identifierType <https://metadata.amagi.tv/skos/amagi_ebu_IdentifierTypeCS#_wikidata>
#                                ] ;
#         ebucoreext:hasAnnotation [ a ebucore:Annotation ;
#                                    ebucore:annotationType ebucore:Topic ;
#                                    ebucore:annotaionConfidence "2.1" ;
#                                   ebucore:annotationId [
#                                       a ebucore:Identifier ;
#                                       ebucore:identifierValue "http://cv.iptc.org/newscodes/mediatopic/20000642" ;
#                                       ebucore:identifierType <https://metadata.amagi.tv/skos/amagi_ebu_IdentifierTypeCS#_iptcmediatopic>
#                                   ] ;
#         ebucoreext:hasEtcAnnotationLocation  <storage://nbcu1234/{assets}/{asset_uri_id}/{asset_artefact_id}/etc_output.json> ; 
#         ebucoreext:hasVideoAnnotationLocation  <storage://nbcu1234/{assets}/{asset_uri_id}/{asset_artefact_id}/videoken_output.json> . 
#        
#       

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#Entity>
        rdf:type             rdfs:Class ;
        rdfs:comment         "To proivde key entites associated with an asset."@en ;
        rdfs:subClassOf      skos:Concept ;
        owl:equivalentClass  <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Keyword> ;
        rdfs:label           "Entity"@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#Category>
        rdf:type         rdfs:Class ;
        rdfs:comment     "A term describing the high level categorization of content covered by the\n            BusinessObject or resource. This is provided as free text in an annotation label or as an identifier pointing to a term in a classification scheme e.g. https://cv.iptc.org/newscodes/mediatopic"@en ;
        rdfs:subClassOf  skos:Concept ;
        rdfs:label       "Subject"@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#AIAugmentedMeta>
        rdf:type         rdfs:Class ;
        rdfs:label       "AIAugmentedMeta"@en ;
        rdfs:comment     "To provide AI augmented metadata for an asset."@en ;
        rdfs:subClassOf  owl:Thing .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#hasAIAugmentedMeta>
        rdf:type      rdf:Property ;
        rdfs:range              <https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#AIAugmentedMeta> ;
        rdfs:domain             <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Asset> ;
        rdfs:label              "hasAIAugmentedMeta"@en ;
        rdfs:comment            "Property to associate an asset with its AI augmented metadata."@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#hasVideoAnnotationLocation>
    rdf:type      rdf:Property ;
    rdfs:domain   <https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#AIAugmentedMeta> ;
    rdfs:range     <http://www.w3.org/2001/XMLSchema#string> ;
    rdfs:comment   "Range: A uri pointing to AI generated video metadata"@en ;
    rdfs:label    "hasVideoAnnotationLocation"@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#hasEtcAnnotationLocation>
    rdf:type      rdf:Property ;
    rdfs:domain   <https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#AIAugmentedMeta> ;
    rdfs:range     <http://www.w3.org/2001/XMLSchema#string> ;
    rdfs:comment   "Range: A uri pointing to AI generated etc(entity, topic, categories) metadata"@en ;
    rdfs:label    "hasEtcAnnotationLocation"@en .


<http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#hasPart>
    rdf:type      rdf:Property ;
    rdfs:domain   <https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#AIAugmentedMeta> ;
    rdfs:comment  "To define Parts (segments, fragments, shots, etc.)\n            within an EditorialObject."@en ;
    rdfs:label    "Part"@en ;
    rdfs:range    <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Part> .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#hasAnnotation> a rdf:Property ;
    rdfs:label      "hasAnnotation"@en ;
    rdfs:comment    "To associate annotation with an Asset"@en ;
    rdfs:domain     <https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#AIAugmentedMeta> ;
    rdfs:range      <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Annotation> .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#VideoAnnotation> a rdfs:Class ;
    rdfs:label      "VideoAnnotation"@en ;
    rdfs:comment    "A class specific to the annotation of a video asset."@en ;
    rdfs:subClassOf <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Annotation> .


######################################################################
# Properties aded for usage restrictions to express limits on the 
# number of times an asset can be published
######################################################################

# Example of usage and publication restrictions
# <https://compass.amagi.tv/gms/entity/A1234> a ebucore:Episode ;
#         ebucoreext:hasPublicationRestrictions [
#                 a ebucoreext:PublicationRestrictions ;
#                 ebucoreext:publicationCap "1"^^xsd:integer ;
#                 ebucoreext:publicationCapPeriod "1D"^^xsd:string ;
#                 ],
#                 [
#                 a ebucoreext:PublicationRestrictions ;
#                 ebucoreext:publicationCap "10"^^xsd:integer ;
#                 ebucoreext:publicationCapPeriod "1W"^^xsd:string ;
#                 ] ;
#         ebucore:hasUsageRestrictions [
#                 a ebucore:UsageRestrictions ;
#                 ebucoreext:maxPublicationEvents "10"^^xsd:integer ;
#                 ] .

# <https://compass.amagi.tv/gms/entity/A1234> a ebucore:Episode ;
#         ebucore:isDistributedOn [ a ebucore:Platform ;
#             ebucore:offers [ a ebucore:PublicationEvent ;
#                     ebucore:hasCoverageRestrictions [ a ebucore:CoverageRestrictions ;
#                             ebucore:rightsEndDateTime "2025-12-03T05:00:00+00:00" ] ] ;
#             ebucore:serviceId "sling" ;
#             ebucore:serviceName "sling" ;
#             ebucore:serviceType "SVOD" ],
#         [ a ebucore:Platform ;
#             ebucore:offers [ a ebucore:PublicationEvent ;
#                     ebucore:hasCoverageRestrictions [ a ebucore:CoverageRestrictions ;
#                             ebucore:rightsEndDateTime "2025-12-03T05:00:00+00:00" ] ;
#                     ebucoreext:hasPublicationRestrictions [
#                                 a ebucoreext:PublicationRestrictions ;
#                                 ebucoreext:publicationCap "1"^^xsd:integer ;
#                                 ebucoreext:publicationCapPeriod "1D"^^xsd:string ;
#                         ],
#                         [
#                                 a ebucoreext:PublicationRestrictions ;
#                                 ebucoreext:publicationCap "10"^^xsd:integer ;
#                                 ebucoreext:publicationCapPeriod "1W"^^xsd:string ;
#                         ] ;
#                         ebucore:hasUsageRestrictions [
#                                a ebucore:UsageRestrictions ;
#                                ebucoreext:maxPublicationEvents "10"^^xsd:integer ;
#                                ] ] ;
#             ebucore:serviceId "apple" ;
#             ebucore:serviceName "apple" ;
#             ebucore:serviceType "SVOD" ] .
        





<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#maxPublicationEvents>
    a               rdf:Property ;
    rdfs:range      <http://www.w3.org/2001/XMLSchema#integer> ;
    rdfs:domain     <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#UsageRestrictions> ;
    rdfs:label      "maxPublicationEvents"@en ;
    rdfs:comment    "To express restriction on the maximum number of publication events for an asset in a given date range."@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#hasPublicationRestrictions>
        rdf:type            rdf:Property ;
        rdfs:range          <https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#PublicationRestrictions> ;
        rdfs:domain         <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#PublicationEvent> ;
        rdfs:domain         <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Asset> ;
        rdfs:comment        "PublicationRestrictions."@en ;
        rdfs:comment        "To express publication restrictions like limits to airing"@en ;
        rdfs:subPropertyOf  <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#isCoveredBy> ;
        rdfs:label          "Publication restrictions"@en ;
        rdfs:range          <http://www.w3.org/2001/XMLSchema#string> .


<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#PublicationRestrictions>
        rdf:type         rdfs:Class ; 
        rdfs:label       "PublicationRestrictions"@en ;
        rdfs:comment     "To express restriction on the number of times an asset can be published."@en ;
        rdfs:subClassOf  <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#UsageRestrictions> .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#publicationCap>
    a               rdf:Property ;
    rdfs:range      <http://www.w3.org/2001/XMLSchema#integer> ;
    rdfs:domain     <https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#PublicationRestrictions> ;
    rdfs:label      "publicationCap"@en ;
    rdfs:comment    "To express restriction on total number of times the media can be published for a given interval(publicationCapPeriod)"@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#publicationCapPeriod>
    a               rdf:Property ;
    rdfs:range      <http://www.w3.org/2001/XMLSchema#string> ;
    rdfs:domain     <https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#PublicationRestrictions> ;
    rdfs:label      "publicationCapPeriod"@en ;
    rdfs:comment    "To express unit time interval on which a given publicationCap value applies to. E.g. 6H, 1M, 2M, 3D, 1W, 10D, 1D6H, 1M15D where H->hours, M is month, D is day, W is week"@en .


# References for SportsEvent
# https://www.wikidata.org/wiki/Wikidata:WikiProject_Sports
# https://schema.org/SportsEvent
<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#SportsEvent>
        rdf:type         rdfs:Class ;
        rdfs:subClassOf  <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Event> ;
        rdfs:label       "Sports event"@en ;
        rdfs:comment     "To describe a sports event."@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension/ebucore#MusicalEvent>
        rdf:type         rdfs:Class ;
        rdfs:subClassOf  <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Event> ;
        rdfs:label       "Musical event"@en ;
        rdfs:comment     "To describe a musical event."@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension/ebucore#PoliticalEvent>
        rdf:type         rdfs:Class ;
        rdfs:subClassOf  <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Event> ;
        rdfs:label       "Political event"@en ;
        rdfs:comment     "To describe a political event, e.g. election campaign, debate etc."@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension/ebucore#superEvent>
        rdf:type      rdf:Property ;
        rdfs:comment  "To identify parent event to which an event belongs to. Equivalent to https://schema.org/superEvent"@en ;
        rdfs:range    <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Event> ;
        rdfs:domain   <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Event> ;
        rdfs:label    "superEvent"@en .


<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#eventNumber>
        rdf:type      rdf:Property ;
        rdfs:domain   <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Event> ;
        rdfs:range    <http://www.w3.org/2001/XMLSchema#string> ;
        rdfs:comment  "The event number in a superEvent. e.g. match number, game number"@en ;
        rdfs:label    "eventNumber"@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension/ebucore#organizer>
        rdf:type      rdf:Property ;
        rdfs:domain   <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Event> ;
        rdfs:range    <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Agent> ;
        rdfs:comment  "Organizer of an event. Equivalent to https://schema.org/organizer. Example: FIFA is organizer of 2014 FIFA World Cup"@en ;
        rdfs:label    "organizer"@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension/ebucore#sponsor>
        rdf:type      rdf:Property ;
        rdfs:domain   <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Event> ;
        rdfs:range    <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Agent> ;
        rdfs:comment  "Sponsor of an event. Equivalent to https://schema.org/sponsor. Example: Adidas AG is sponsor of 2014 FIFA World Cup"@en ;
        rdfs:label    "sponsor"@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#hasParticipatingTeam>
        rdf:type      rdf:Property ;
        rdfs:comment  "To identify participating Agents."@en ;
        rdfs:range    <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Team> ;
        rdfs:range    <http://www.w3.org/2001/XMLSchema#string> ;
        rdfs:label    "Participating Team"@en ;
        rdfs:comment  "range: Agent or string"@en ;
        rdfs:domain   <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#SportsEvent> .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension/ebucore#hasEventGenre>
        rdf:type      rdf:Property ;
        rdfs:range    <http://www.w3.org/2001/XMLSchema#string> ;
        rdfs:label    "Event genre"@en ;
        rdfs:domain   <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Event> ;
        rdfs:comment  "The genre of content associated with the Event."@en ;
        rdfs:range    <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Genre> ;
        rdfs:comment  "Range: string or Genre"@en .

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#hasEventRelatedPicture>
        rdf:type        rdf:Property ;
        rdfs:label      "Event Related picture"@en ;
        rdfs:domain     <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Event> ;
        rdfs:range      <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Picture> ;
        rdfs:comment  "A link/locator to a picture related to an Event."@en .


# Graphics

<https://metadata.amagi.tv/ebu/amagi_ebucoreExtension#Graphic>
        rdf:type         rdfs:Class ;
        rdfs:label       "Graphic"@en ;
        rdfs:comment     "Entity to represent the graphic to be used as overlay"@en ;
        rdfs:subClassOf  <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#EditorialObject> .