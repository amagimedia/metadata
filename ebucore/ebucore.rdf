    <rdfs:comment xml:lang="en">To identify and describe Props used in productions (e.g. vehicles, objects of various shapes and brand and purpose, etc.).</rdfs:comment>
    <rdfs:label xml:lang="en">Props</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Artefact"/>
  </rdfs:Class>
  <rdf:Property rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#orderedFlag">
