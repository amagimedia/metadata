  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#EditorialObject_Type">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en">Editorial object type</rdfs:label>
    <rdfs:comment xml:lang="en">To define a type of editorial object.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Emotion_Type">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:comment xml:lang="en">To define a type of emotion.</rdfs:comment>
    <rdfs:label xml:lang="en">Emotion type</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#VideoCodec">
    <rdfs:label xml:lang="en">Video codec</rdfs:label>
    <rdfs:comment xml:lang="en">To provide information about a video codec.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Codec"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#AncillaryDataFormat">
    <rdfs:label xml:lang="en">Ancillary data format</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#DataFormat"/>
    <rdfs:comment xml:lang="en">To define the format of AncillaryData such as
            legacy data used to be carried in vertical blanking intervals. This is provided as free
            text in an annotation label or as an identifier pointing to a term in a classification
            scheme.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#CoverageRestrictions">
    <rdfs:comment xml:lang="en">To provide information on possible restrictions
            regarding the temporal and spatial coverage for publication.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Rights"/>
    <rdfs:label xml:lang="en">Coverage restrictions</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Topic">
    <rdfs:label xml:lang="en">Topic</rdfs:label>
    <rdfs:comment xml:lang="en">A type subject for use in some contexts. This
            is provided as free text in an annotation label or as an identifier pointing to a term
            in a classification scheme.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#NewsItem">
    <rdfs:label xml:lang="en">News Item</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Item"/>
    <rdfs:comment xml:lang="en">A NewsItem aggregates all information about a particular news event.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.bbc.co.uk/ontologies/coreconcepts/Organisation">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en">E.g., Businesses.</rdfs:comment>
    <rdfs:label xml:lang="en">Organisation</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Biography">
    <rdfs:subClassOf rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#BibliographicalObject"/>
    <rdfs:comment xml:lang="en">To record a biography.</rdfs:comment>
    <rdfs:label xml:lang="en">Biography</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Relation">
    <rdfs:comment xml:lang="en">To define links and relations.</rdfs:comment>
    <rdfs:label xml:lang="en">Relation</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#EncodingFormat">
    <rdfs:subClassOf rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Format"/>
    <rdfs:label xml:lang="en">Encoding</rdfs:label>
    <rdfs:comment xml:lang="en">To provide a definition of the encoding format
            for audio and video. This is provided as free text in an annotation label or as an
            identifier pointing to a term in a classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_AudioCompressionCodeCS.rdf or
            http://www.ebu.ch/metadata/ontologies/skos/ebu_VideoCompressionCodeCS.rdf.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Track">
    <rdfs:label xml:lang="en">Track</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#MediaResource"/>
    <rdfs:comment xml:lang="en">Audiovisual content can be composed of audio,
            video and data Tracks (including captioning and subtitling).</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.bbc.co.uk/ontologies/coreconcepts/Theme">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:label xml:lang="en">Theme</rdfs:label>
    <rdfs:comment xml:lang="en">This concept is used to represent intangible things or abstract concepts. For example, Financial Crisis, Midwifery, Happiness.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Image">
    <rdfs:label xml:lang="en">Image</rdfs:label>
    <rdfs:comment xml:lang="en">A still image / thumbnail / key frame / logo
            related to the media resource or being the media resource itself.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#BusinessObject"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#ObjectType">
    <rdfs:subClassOf rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Type"/>
    <rdfs:label xml:lang="en">Object type</rdfs:label>
    <rdfs:comment xml:lang="en">To specify the type of BusinessObject e.g. and
            EditorialObject of type "programme" or clip". This is
            provided as free text in an annotation label or as an identifier pointing to a term in a
            classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_ObjectTypeCodeCS.rdf.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#EventType">
    <rdfs:label xml:lang="en">Event type</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:comment xml:lang="en">To define a type of event.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Subtitling">
    <rdfs:subClassOf rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Track"/>
    <rdfs:comment xml:lang="en">To signal the presence of subtitles for
            translation in alternative languages.</rdfs:comment>
    <rdfs:label xml:lang="en">Subtitling</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#PhysicalResource">
    <rdfs:comment xml:lang="en">To describe a physical resource e.g. a tape.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#MediaResource"/>
    <rdfs:label xml:lang="en">Physical resource</rdfs:label>
  </rdfs:Class>
