    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en">To identify an animal.</rdfs:comment>
    <rdfs:label xml:lang="en">Animal</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#Subject">
    <rdfs:comment xml:lang="en">A term describing the topic covered by the
            BusinessObject or resource. This is provided as free text in an annotation label or as
            an identifier pointing to a term in a classification scheme.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en">Subject</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#FoodStyle">
    <rdfs:label xml:lang="en">Food style</rdfs:label>
    <rdfs:comment xml:lang="en">To define a style of food.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdfs:Class>
  <rdf:Property rdf:about="http://purl.org/dc/elements/1.1/subject">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en">To associate a Subject or topic addressed by a&#13;
            BusinessObject / Resource.</rdfs:comment>
